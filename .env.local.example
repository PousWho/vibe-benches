# Карта Mapbox (уже есть)
NEXT_PUBLIC_MAPBOX_TOKEN=твой_mapbox_token

# Supabase (добавь после создания проекта на supabase.com)
NEXT_PUBLIC_SUPABASE_URL=https://xxxxx.supabase.co
